[
  {
    "cpu": 128,
    "essential": true,
    "image": "${image_url}",
    "memory": 256,
    "name": "${container_name}",
    "portMappings": [
      {
        "containerPort": 3333,
        "hostPort": 0
      }
    ],
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "${log_group_name}",
            "awslogs-region": "${log_group_region}"
        }
    }

  }
]
